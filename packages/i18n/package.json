{
	"name": "@288-toolkit/i18n",
	"version": "8.1.0",
	"scripts": {
		"build": "svelte-package",
		"test": "vitest run",
		"test:watch": "vitest",
		"format:prettier": "prettier --config ./.prettierrc",
		"format:file": "npm run format:prettier -- --write",
		"format": "npm run format:file .",
		"check": "svelte-check --tsconfig ./tsconfig.json --fail-on-warnings",
		"check:watch": "npm run check -- --watch",
		"lint": "eslint --max-warnings=0 --cache .",
		"lint:fix": "npm run lint -- --fix",
		"lc": "npm run lint && npm run check",
		"lc:watch": "npm run lint && npm run check -- --watch"
	},
	"author": "DeuxHuitHuit",
	"license": "MIT",
	"type": "module",
	"files": [
		"./dist"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./types": {
			"types": "./dist/types/index.d.ts"
		},
		"./handles": {
			"types": "./dist/handles/index.d.ts",
			"default": "./dist/handle/index.js"
		},
		"./translations/server": {
			"types": "./dist/translations/server/index.d.ts",
			"svelte": "./dist/translations/server/index.js"
		},
		"./translations/client": {
			"types": "./dist/translations/client/index.d.ts",
			"svelte": "./dist/translations/client/index.js"
		}
	},
	"peerDependencies": {
		"@sveltejs/kit": "2.x",
		"svelte": "4.x || 5.x"
	},
	"dependencies": {
		"@288-toolkit/http": "workspace:*",
		"@288-toolkit/types": "workspace:*",
		"esm-env": "1.0.0",
		"just-safe-get": "4.2.0"
	}
}
